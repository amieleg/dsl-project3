A := 440
C := A * 2^(-9/12)
C# := A * 2^(-8/12)
D := A * 2^(-7/12)
D# := A * 2^(-6/12)
E := A * 2^(-5/12)
F := A * 2^(-4/12)
F# := A * 2^(-3/12)
G := A * 2^(-2/12)
G# := A * 2^(-1/12)
A# := A * 2^(1/12)
B := A * 2^(2/12)
B# := A * 2^(3/12)

func C_Chord(t) begin
    Sine(t, C) + Sine(t, E) + Sine(t, G)
end

func G_Chord(t) begin
    Sine(t, G) + Sine(t, B) + Sine(t, D)
end

func Combined(t) begin
    if t < 1 then
        C_Chord(t)
    else
        G_Chord(t)
    end 
end

output Combined(t) 2